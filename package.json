{
  "name": "fbself-m",
  "version": "0.0.1",
  "description": "Interact with Facebook using user accounts (self-bot) through mini/mobile version (m.facebook)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "npm run build && npm pack",
    "test": "mocha --require ts-node/register --timeout 15000 test/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BadAimWeeb/fbself-m.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "facebook",
    "api",
    "self",
    "bot",
    "fb"
  ],
  "author": "BadAimWeeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BadAimWeeb/fbself-m/issues"
  },
  "homepage": "https://github.com/BadAimWeeb/fbself-m#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.3",
    "@types/mocha-steps": "^1.3.0",
    "@types/msgpack5": "^3.4.2",
    "@types/node-fetch": "^2.5.11",
    "@types/speakeasy": "^2.0.6",
    "@types/tough-cookie": "^4.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.0.2",
    "mocha-steps": "^1.3.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "fetch-h2": "^3.0.0",
    "mqtt": "^4.2.8",
    "msgpack5": "^5.3.2",
    "node-fetch": "^2.6.1",
    "speakeasy": "^2.0.0",
    "tough-cookie": "^4.0.0"
  }
}
